@article{Seg_Livres_Heures,
	author = {DAILLE, B{\'e}atrice and HAZEM, Amir and KERMORVANT, Christopher and MAARAND, Martin and BONHOMME, Marie-Laurence and STUTZMANN, Dominique and CURRIE, Jacob and JACQUIN, Christine},
